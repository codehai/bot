import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.*;
import org.sikuli.Screen;

public class open{
	public static void main(String[ ] args){
		FirefoxProfile profile = new FirefoxProfile();
		profile.setPreference("general.useragent.override", "whatever you want");//useragent
		WebDriver driver = new FirefoxDriver(profile);
		Screen s  =  new Screen();
		String imgpath = "/home/hao/桌面/selenium/";
		driver.get("http://www.baidu.com");
		s.type(imgpath+"search2.jpg","sikuli");
		//s.click(imgpath+"sreach_input.jpg");
	}
}